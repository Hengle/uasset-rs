    "version": "0.2.0",
    "configurations": [
        {
            "type": "lldb",
            "request": "launch",
            "name": "Debug executable 'uasset'",
            "cargo": {
                "args": [
                    "build",
                    "--bin=uasset",
                    "--package=uasset",
                    "--features=commandline-tool"
                ],
                "filter": {
                    "name": "uasset",
                    "kind": "bin"
                }
            },
            "args": [
                "assets/UE410/SimpleRefs/SimpleRefsRoot.uasset",
                "assets/UE426/SimpleRefs/SimpleRefsRoot.uasset"
            ],
            "cwd": "${workspaceFolder}"
        },
        {
            "type": "lldb",
            "request": "launch",
            "name": "Debug basic_parsing tests in library 'uasset'",
            "cargo": {
                "args": [
                    "test",
                    "--no-run",
                    "--tests",
                    "--package=uasset"
                ],
                "filter": {
                    "name": "basic_parsing",
                    "kind": "test"
                }
            },
            "args": [],
            "cwd": "${workspaceFolder}"
        },
        {
            "type": "lldb",
            "request": "launch",
            "name": "Debug asset_references tests in library 'uasset'",
            "cargo": {
                "args": [
                    "test",
                    "--no-run",
                    "--tests",
                    "--package=uasset"
                ],
                "filter": {
                    "name": "asset_references",
                    "kind": "test"
                }
            },
            "args": [],
            "cwd": "${workspaceFolder}"
        },
    ]
}